{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "note-secretary",
  "version": "0.1.0",
  "identifier": "top.vuhe.note-secretary",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../node_modules/@shikijs/langs/dist/*.mjs": "shiki-langs/",
      "../node_modules/mermaid/dist/mermaid.esm.min.mjs": "mermaid/mermaid.esm.min.mjs",
      "../node_modules/mermaid/dist/chunks/mermaid.esm.min/*.mjs": "mermaid-chunks/",
      "../node_modules/katex/dist/katex.min.css": "katex/katex.min.css",
      "../node_modules/katex/dist/fonts/*": "katex-fonts/"
    }
  }
}
