{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "note-secretary",
  "version": "0.1.0",
  "identifier": "top.vuhe.note-secretary",
  "build": {
    "beforeDevCommand": "bun dev:next",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "bun build:next",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
